    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/qs": "^6.5.2",
    "@types/react-dom": "^16.8.2",
    "@types/react-loadable": "^5.5.0",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/recompose": "^0.30.5",
    "@types/validator": "^10.9.0",
